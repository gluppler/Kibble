{
  "name": "kibble",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run generate:icons && prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:test": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/test-db-connection.ts",
    "db:cleanup": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/cleanup-database.ts",
    "db:cleanup:apply": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/cleanup-database.ts --apply",
    "generate:icons": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/generate-icons.ts",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^6.19.0",
    "@types/bcrypt": "^5.0.2",
    "@types/qrcode": "^1.5.6",
    "@types/react-datepicker": "^6.2.0",
    "bcrypt": "^6.0.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.554.0",
    "next": "^16.0.4",
    "next-auth": "^5.0.0-beta.25",
    "otplib": "^12.0.1",
    "prisma": "^6.19.0",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-datepicker": "^8.10.0",
    "react-dom": "^19.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/sharp": "^0.31.1",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/ui": "^4.0.14",
    "autoprefixer": "^10.4.22",
    "baseline-browser-mapping": "latest",
    "browserslist": "^4.28.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.4",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.18",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.14"
  }
